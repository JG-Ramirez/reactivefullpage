{
  "name": "@codegabo/reactivefullpage",
  "version": "1.0.3",
  "description": "react wrapper for your full pages layouts",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "rollup --config"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jg-rc/reactivefullpage.git"
  },
  "keywords": [
    "fullpage",
    "slider"
  ],
  "author": "jgrc",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jg-rc/reactivefullpage/issues"
  },
  "homepage": "https://github.com/jg-rc/reactivefullpage#readme",
  "dependencies": {
    "@types/react": "^16.9.35",
    "react": "^16.13.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.8.3",
    "@babel/preset-react": "^7.9.4",
    "@rollup/plugin-babel": "^5.0.2",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "typescript": "^3.7.5",
    "rollup": "^2.10.8"
  }
}
